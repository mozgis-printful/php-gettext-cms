{
    "name": "printful/php-gettext-cms",
    "description": "CMS backend for Gettext translation management",
    "type": "library",
    "require": {
        "php": ">=7.0",
        "gettext/gettext": "dev-master",
        "league/flysystem": "^1.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^6.0",
        "mockery/mockery": "^1.1",
        "mikey179/vfsStream": "^1.6"
    },
    "repositories": [
        {
            "type": "git",
            "url": "https://github.com/briedis/Gettext.git"
        }
    ],
    "autoload": {
        "psr-4": {
            "Printful\\GettextCms\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Printful\\GettextCms\\Tests\\": "tests/"
        }
    },
    "minimum-stability": "stable"
}
