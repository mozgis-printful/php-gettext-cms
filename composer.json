{
    "name": "printful/php-gettext-cms",
    "description": "CMS backend for Gettext translation management",
    "type": "library",
    "require": {
        "php": ">=7.0",
        "ext-json": "*",
        "ext-zip": "*",
        "ext-gettext": "*",
        "gettext/gettext": "^4.6.0",
        "league/flysystem": "^1.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^6.0",
        "mockery/mockery": "^1.1",
        "mikey179/vfsStream": "^1.6"
    },
    "autoload": {
        "psr-4": {
            "Printful\\GettextCms\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Printful\\GettextCms\\Tests\\": "tests/"
        }
    },
    "minimum-stability": "stable"
}
